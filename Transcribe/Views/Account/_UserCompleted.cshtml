@model IEnumerable<Transcribe.Models.CompletedViewModel>

<h3>Completed <span class="badge">@Model.Count()</span></h3>
<hr />
<div id="completed">
    @if (Model.Count() > 0)
    {
        foreach (var item in Model)
        {
            <p><a href="/Transcribe/Index/@item.Completed.RecordId">@item.Records.title</a></p>
        }
    }
    else
    {
        <p>You have no completed transcriptions</p>
    }
</div>
